{
  "name": "brainshell",
  "version": "1.0.0",
  "description": "A magical wordprocessor for your brain",
  "main": "server/server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "node tests/parsing",
    "build-parser": "node tools/compile.js src/parser.ometajs > parser_compiled.js",
    "pretest": "node tools/compile.js src/parser.ometajs > parser_compiled.js",
    "server": "node server/server.js",
    "build-ui": "browserify --transform reactify ui/main.jsx node_modules/appy-style/react/CustomList.jsx -o ui/bundle.js --debug --verbose ",
    "build-react": "browserify -r react -o ui/tallycat/react_bundle.js",
    "build-tallyui": "browserify --transform reactify ui/tallycat/tally.jsx --exclude react --ignore colors/safe -o ui/tallycat/bundle.js",
    "watch-ui": "watchify --transform reactify ui/main.jsx node_modules/appy-style/react/CustomList.jsx -o ui/bundle.js --debug --verbose "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshmarinacci/brainshell.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joshmarinacci/brainshell/issues"
  },
  "homepage": "https://github.com/joshmarinacci/brainshell",
  "dependencies": {
    "@joshmarinacci/jsonstream": "^1.0.5",
    "@joshmarinacci/ometa-js": "^1.3.5",
    "appy-style": "^0.0.2",
    "body-parser": "^1.12.3",
    "compression": "^1.4.4",
    "cors": "^2.6.0",
    "csv-parse": "^0.1.1",
    "csv-streamify": "^2.0.0",
    "event-stream": "^3.3.0",
    "express": "^4.12.3",
    "font-awesome": "^4.3.0",
    "moment": "^2.10.2",
    "ndjson": "^1.3.0",
    "nedb": "^1.1.2",
    "nodejs-websocket": "^1.2.1",
    "q": "^1.3.0",
    "react": "^0.13.2",
    "reactify": "^1.1.0",
    "request": "^2.55.0",
    "tape": "^4.0.0",
    "twitter": "^1.2.5"
  }
}
